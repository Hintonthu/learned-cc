{
 "BaseModel": {
  "args": {
   "size_for_cifar": false
  },
  "class": "base_models.resnet.ResNet18"
 },
 "Decoder": {
  "class": "coders.mlp_coder.MLPDecoder"
 },
 "DecoderOptimizer": {
  "args": {
   "lr": 0.001,
   "weight_decay": 1e-05
  },
  "class": "torch.optim.Adam"
 },
 "Encoder": {
  "args": {
   "intermediate_channels": 20
  },
  "class": "coders.conv_encoder.ConvEncoder"
 },
 "EncoderOptimizer": {
  "args": {
   "lr": 0.001,
   "weight_decay": 1e-05
  },
  "class": "torch.optim.Adam"
 },
 "Loss": {
  "class": "torch.nn.CrossEntropyLoss"
 },
 "TrainDataset": {
  "args": {
   "is_train": true
  },
  "class": "datasets.code_dataset.MNISTCodeDataset"
 },
 "ValidationDataset": {
  "args": {
   "is_train": false
  },
  "class": "datasets.code_dataset.MNISTCodeDataset"
 },
 "base_model_file": "base_model_trained_files/mnist/resnet18/model.t7",
 "base_model_input_size": [
  -1,
  1,
  28,
  28
 ],
 "batch_size": 64,
 "ec_k": 2,
 "ec_r": 1,
 "final_epoch": 500,
 "save_dir": "save/mnist/resnet18/k2/xent-label/conv"
}