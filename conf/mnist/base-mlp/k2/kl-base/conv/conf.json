{
 "BaseModel": {
  "class": "base_models.base_mlp.BaseMLP"
 },
 "Dataset": {
  "class": "datasets.code_dataset.MNISTCodeDataset"
 },
 "Decoder": {
  "class": "coders.mlp_coder.MLPDecoder"
 },
 "DecoderOptimizer": {
  "args": {
   "lr": 0.001,
   "weight_decay": 1e-05
  },
  "class": "torch.optim.Adam"
 },
 "Encoder": {
  "args": {
   "intermediate_channels": 20
  },
  "class": "coders.conv_encoder.ConvEncoder"
 },
 "EncoderOptimizer": {
  "args": {
   "lr": 0.001,
   "weight_decay": 1e-05
  },
  "class": "torch.optim.Adam"
 },
 "Loss": {
  "class": "loss.kldiv.KLDivLoss"
 },
 "base_model_file": "base_model_trained_files/mnist/base-mlp/model.t7",
 "base_model_input_size": [
  -1,
  784
 ],
 "batch_size": 64,
 "ec_k": 2,
 "ec_r": 1,
 "final_epoch": 500,
 "save_dir": "save/mnist/base-mlp/k2/kl-base/conv"
}